{
  "name": "laundry-cfn",
  "version": "1.5.0",
  "description": "A CloudFormation template linter.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "bin": "bin/laundry",
  "license": "MIT",
  "homepage": "https://github.com/unbounce/laundry",
  "author": "james@unbounce.com",
  "keywords": [
    "cloudformation",
    "aws",
    "lint"
  ],
  "contributors": [
    "james@unbounce.com",
    "tavis@unbounce.com"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/unbounce/laundry.git"
  },
  "bugs": {
    "url": "https://github.com/unbounce/laundry/issues"
  },
  "engines": {
    "node": ">= 8.0.0",
    "npm": ">= 5.0.0"
  },
  "scripts": {
    "start": "jest --watch",
    "build": "tsc -p .",
    "test": "jest",
    "update-resource-specification": "curl --output src/specs/CloudFormationResourceSpecification.json https://d1uauaxba7bl26.cloudfront.net/latest/CloudFormationResourceSpecification.json",
    "version": "auto-changelog -p && git add CHANGELOG.md",
    "postversion": "git push && git push --tags",
    "prepublishOnly": "npm run clean && npm install && npm test && npm run build",
    "clean": "rm -rf node_modules lib"
  },
  "devDependencies": {
    "@types/jest": "24.0.19",
    "@types/js-yaml": "3.12.1",
    "@types/lodash": "4.14.122",
    "@types/node": "12.11.7",
    "@types/yargs": "13.0.3",
    "auto-changelog": "1.16.1",
    "jest": "24.9.0",
    "ts-jest": "24.1.0",
    "typescript": "3.6.4"
  },
  "dependencies": {
    "didyoumean2": "3.1.2",
    "js-yaml": "^3.12.0",
    "lodash": "4.17.19",
    "yargs": "14.2.0"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test))\\.(jsx?|tsx?)$",
    "testPathIgnorePatterns": [
      "/node_modules/",
      ".*flycheck.*",
      ".*\\.d\\.ts"
    ],
    "watchPathIgnorePatterns": [
      ".*flycheck.*"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  }
}
